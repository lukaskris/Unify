<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <path
                android:name="path"
                android:pathData="M 12 2 C 9.349 2 6.804 3.054 4.929 4.929 C 3.054 6.804 2 9.349 2 12 C 2 14.651 3.054 17.196 4.929 19.071 C 6.804 20.946 9.349 22 12 22 C 14.651 22 17.196 20.946 19.071 19.071 C 20.946 17.196 22 14.651 22 12 C 22 9.349 20.946 6.804 19.071 4.929 C 17.196 3.054 14.651 2 12 2 Z"
                android:strokeColor="@color/Unify_NN900"
                android:strokeWidth="2"/>
            <group
                android:name="group"
                android:pivotX="12"
                android:pivotY="12">
                <path
                    android:name="inside"
                    android:pathData="M 12 6 C 10.409 6 8.882 6.633 7.757 7.757 C 6.633 8.882 6 10.409 6 12 C 6 13.591 6.633 15.118 7.757 16.243 C 8.882 17.367 10.409 18 12 18 C 13.591 18 15.118 17.367 16.243 16.243 C 17.367 15.118 18 13.591 18 12 C 18 10.409 17.367 8.882 16.243 7.757 C 15.118 6.633 13.591 6 12 6 Z"
                    android:fillColor="@color/Unify_NN900"
                    android:strokeWidth="1"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="strokeColor"
                android:duration="@integer/Unify_T2"
                android:valueFrom="@color/Unify_GN500"
                android:valueTo="@color/Unify_NN900"
                android:valueType="colorType"
                android:interpolator="@anim/unify_ease_out_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:duration="@integer/Unify_T2"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@anim/unify_ease_out_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="@integer/Unify_T2"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@anim/unify_ease_out_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="inside">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillColor"
                android:duration="@integer/Unify_T2"
                android:valueFrom="@color/Unify_GN500"
                android:valueTo="@color/Unify_NN900"
                android:valueType="colorType"
                android:interpolator="@anim/unify_ease_out_interpolator"/>
        </aapt:attr>
    </target>
</animated-vector>
