<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <item app:chipAlternateMedium="true">
        <aapt:attr name="android:drawable">
            <shape xmlns:android="http://schemas.android.com/apk/res/android"
                android:shape="rectangle" android:padding="0dp">
                <solid android:color="@color/chipsunify_background_enable" />
                <corners
                    android:bottomLeftRadius="16dp"
                    android:bottomRightRadius="16dp"
                    android:topLeftRadius="16dp"
                    android:topRightRadius="16dp" />
                <stroke android:color="@color/chipsunify_border_selected" android:width="1dp" />
            </shape>
        </aapt:attr>
    </item>
    <item app:chipAlternateSmall="true">
        <aapt:attr name="android:drawable">
