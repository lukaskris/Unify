<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Label">
        <attr name="labelType" format="enum">
            <enum name="general_dark_grey" value="1" />
            <enum name="general_dark_green" value="2" />
            <enum name="general_dark_blue" value="3" />
            <enum name="general_dark_orange" value="4" />
            <enum name="general_dark_red" value="5" />
            <enum name="general_light_red" value="6" />
            <enum name="general_light_green" value="7" />
            <enum name="general_light_blue" value="8" />
            <enum name="general_light_grey" value="9" />
            <enum name="general_light_orange" value="10" />
            <enum name="time_high_priority" value="11" />
            <enum name="time_low_priority" value="12" />
            <enum name="time_medium_priority" value="13" />
        </attr>
        <attr name="labelTitle" format="string" />
        <attr name="labelTimerIcon" format="boolean" />
        <attr name="labelOpacityLevel" format="float" />
    </declare-styleable>

    <declare-styleable name="LabelProgress">
        <attr name="labelProgressTitle" format="string" />
        <attr name="labelProgress" format="integer" />
        <attr name="labelColor" format="enum">
            <enum name="general_light_red" value="6" />
            <enum name="general_light_green" value="7" />
            <enum name="general_light_blue" value="8" />
            <enum name="general_light_grey" value="9" />
            <enum name="general_light_orange" value="10" />
        </attr>
        <attr name="labelTypographyType" format="enum">
            <enum name="heading_1" value="1" />
            <enum name="heading_2" value="2" />
            <enum name="heading_3" value="3" />
            <enum name="heading_4" value="4" />
            <enum name="heading_5" value="5" />
            <enum name="heading_6" value="6" />
            <enum name="body_1" value="7" />
            <enum name="body_2" value="8" />
            <enum name="body_3" value="9" />
            <enum name="small" value="10" />
        </attr>
    </declare-styleable>

</resources>